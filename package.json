{
  "name": "garn-validator",
  "version": "0.0.3",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "docker-compose run node",
    "test": "cd src && npm run test:watch",
    "test:watch": "jest src --watchAll",
    "postbuild": "npm run test:dist",
    "patch": "npm version patch",
    "postpatch": "git push origin master --tags",
    "prebuild": "npm run patch",
    "build": "npm run _build",
    "env": "NODE_ENV=production || set NODE_ENV=production",
    "_build": "rimraf dist && npm run env && babel src --out-dir dist --ignore \"src/*.test.js\"",
    "predeploy": "npm run build",
    "deploy": "npm run commit && git push origin master",
    "postdeploy": "npm run publi",
    "publi": "npm publish",
    "prepubli": "npm whoami || npm login",
    "inject": "node makeReadme.js",
    "commit": "git add . && git commit -m 'build done'",
    "precommit": "npm run inject"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.0.6",
    "rimraf": "^3.0.0",
    "jest": "^24.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jupegarnica/garn-validator.git"
  },
  "keywords": [],
  "author": "jupegarnica",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jupegarnica/garn-validator/issues"
  },
  "homepage": "https://github.com/jupegarnica/garn-validator#readme"
}
